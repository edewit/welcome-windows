<Application
    x:Class="Welcome_Project_Windows.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Welcome_Project_Windows">

    <Application.Resources>
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
    			<ResourceDictionary Source="ms-appx:///Platform.xaml"/>
    		</ResourceDictionary.MergedDictionaries>
    		<FontFamily x:Key="FontAwesome">/Library/fontawesome-webfont.ttf#FontAwesome</FontFamily>
    	</ResourceDictionary>
    </Application.Resources>
    
</Application>
